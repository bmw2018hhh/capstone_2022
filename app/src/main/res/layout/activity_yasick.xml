<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/yasickcontent">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/night_imgview" />

    <ImageButton
        android:id="@+id/yasickplus"
        android:layout_width="114dp"
        android:layout_height="110dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="248dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_add_24"
        tools:ignore="SpeakableTextPresentCheck" />


    <TextView
        android:id="@+id/ytextView"
        android:layout_width="283dp"
        android:layout_height="56dp"
        android:layout_marginStart="170dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="198dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/yasickplus"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menutextView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/menutextView"
        android:layout_width="172dp"
        android:layout_height="30dp"
        android:layout_marginTop="385dp"
        android:layout_marginEnd="-30dp"
        android:layout_marginBottom="91dp"
        android:textColor="@color/black"
        android:textSize="18dp"
        app:layout_constraintBottom_toTopOf="@+id/ytextView"
        app:layout_constraintEnd_toEndOf="@+id/yasickplus"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>